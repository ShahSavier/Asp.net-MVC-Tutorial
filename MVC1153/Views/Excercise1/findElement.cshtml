@model MVC1153.Models.Employee
<h3>
    Find By Elements
</h3>
<p>
    Employee Id : @Model.EmpId
</p>
<p>
    Name : @Model.Name
</p>
<p>
    Gender : @Model.Gender
</p>
<p>
    Position : @Model.Position
</p>
<p>
    Start Date : @Model.StartDate.ToLongDateString()
</p>
<p>
    Campus : @Model.Campus
</p>
<p>
    Salary : @Model.Salary.ToString("c2")
</p>