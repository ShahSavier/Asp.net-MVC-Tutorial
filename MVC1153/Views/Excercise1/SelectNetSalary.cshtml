@model IEnumerable<MVC1153.Models.Employee>

<h2> Example 8 </h2>
<br /><br />
<h3> List of Employees</h3>
<br />
<table border="1" width="100%">
    <tr>
        <th>#</th>
        <th>Emp</th>
        <th>Name</th>
        <th>Position</th>
        <th>Salary</th>
        <th>Decuction</th>
        <th>Net Salary</th>
    </tr>
    @{ int row = 1;}
    @foreach (var employee in Model)
    {
<tr>
    <td>@row</td>
    <td>@employee.EmpId</td>
    <td>@employee.Name</td>
    <td>@employee.Position</td>
    <td>@employee.Salary.ToString("c2")</td>
    <td>@employee.Deduction.ToString("c2")</td>
    <td>@employee.NetSalary.ToString("c2")</td>

</tr>
    }
</table>